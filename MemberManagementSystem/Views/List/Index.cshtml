@model IEnumerable<MemberManagementSystem.Models.Employees>

@{
    ViewBag.Title = "Employee List";
}

<h2>Employee List</h2>

<p>
    @Html.ActionLink("Create New Employee", "Index", "Create")
</p>
<table class="table" style="float:left;">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Age)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tel)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tel)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", "Edit", new { id = item.EmployeeID }, "") |

                @Ajax.ActionLink("Edit", "Edit", "Edit", new { id = item.EmployeeID },
                    new AjaxOptions { UpdateTargetId = "Edit", InsertionMode = InsertionMode.Replace, HttpMethod = "POST" }) |
                @Html.ActionLink("Delete", "Delete", "Delete", new { id = item.EmployeeID }, new { onclick = "return confirm('Are You Sure To Delete Employee " + item.Name + " ?');" })
            </td>
        </tr>
    }
</table>

